	public int run() {
		
	}
	
	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		Job job1 = Job.getInstance(getConf());
		
		FileInputFormat.setInputPaths(job1, new Path[0]);
		
		
	}

}
